    <DTS:Variable
      DTS:CreationName=""
      DTS:DTSID="{87A384EA-93F1-4DC9-86B0-D18DDA75CEA0}"
      DTS:EvaluateAsExpression="True"
      DTS:Expression="&quot;declare @ParentPoolId int, @PoolId int&#xA;&#xA;select @PoolId=PoolId, @ParentPoolId=ParentPoolId from config.PoolHeader&#xA;where PoolId=&quot;+ @[User::PoolId] +&quot;&#xA;&#xA;while @ParentPoolId!=0 and @ParentPoolId is not null&#xA;begin&#xA;&#x9;select @PoolId=PoolId, @ParentPoolId=ParentPoolId from config.PoolHeader&#xA;&#x9;where PoolId=@ParentPoolId&#xA;end&#xA;&#xA;&#xA;select PoolDBName from config.PoolHeader where Poolid=@PoolId&quot;"
      DTS:IncludeInDebugDump="2345"
      DTS:Namespace="User"
      DTS:ObjectName="V_GetTargetSqlConnection">
      <DTS:VariableValue
        DTS:DataType="8">declare @ParentPoolId int, @PoolId int

select @PoolId=PoolId, @ParentPoolId=ParentPoolId from config.PoolHeader
where PoolId=60714

while @ParentPoolId!=0 and @ParentPoolId is not null
begin
	select @PoolId=PoolId, @ParentPoolId=ParentPoolId from config.PoolHeader
	where PoolId=@ParentPoolId
end


select PoolDBName from config.PoolHeader where Poolid=@PoolId</DTS:VariableValue>
