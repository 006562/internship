<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <security>
            <requestFiltering>
                <requestLimits maxUrl="65536" maxQueryString="65536" maxAllowedContentLength="614572800">
                    <headerLimits>
                        <add header="Content-type" sizeLimit="65576" />
                    </headerLimits>
                </requestLimits>
            </requestFiltering>
        </security>
    </system.webServer>
  <system.web>
    <httpRuntime maxQueryStringLength="327680" maxUrlLength="655360" maxRequestLength="6553600" enable="true"/>
  </system.web>
</configuration>
